{
  "name": "@cmcleese/cesium-navigation",
  "version": "4.0.7",
  "description": "Cesium plugin that adds a compass, navigator, and distance scale to the map",
  "keywords": [
    "cesium",
    "compass",
    "distance",
    "navigation",
    "navigator",
    "scale"
  ],
  "homepage": "https://github.com/cmcleese/cesium-navigation#readme",
  "bugs": {
    "url": "https://github.com/cmcleese/cesium-navigation/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cmcleese/cesium-navigation.git"
  },
  "license": "Apache-2.0",
  "author": "Alberto Acevedo",
  "main": "dist/index.js",
  "module": "dist/esm.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "prebuild": "yarn sort-package-json & yarn clean",
    "build": "rollup -c --bundleConfigAsCjs",
    "build:all": "yarn clean && yarn build && yarn build:example",
    "build:example": "webpack --config Examples/webpack.config.js ",
    "clean": "rm -rf dist Examples/dist",
    "clean:example": "rm -rf Examples/dist",
    "format": "prettier-eslint --config --write '{Source,test,Test}/**/*.js'",
    "lint": "eslint --fix --no-error-on-unmatched-pattern '{Source,Test,test}/**/*.js'",
    "prepublishOnly": "yarn build:all",
    "s": "yarn serve",
    "serve": "yarn http-server .",
    "start": "yarn s & yarn rollup -c -w",
    "pretest": "yarn build",
    "test": "yarn lint"
  },
  "dependencies": {
    "hammerjs": "^2.0.8",
    "knockout": "^3.5.1",
    "knockout-es5": "^0.4.5",
    "markdown-it": "^13.0.1",
    "markdown-it-sanitizer": "^0.4.3"
  },
  "devDependencies": {
    "@turf/helpers": "^6.5.0",
    "@znemz/cesium-webpack-config": "0.1.2",
    "@znemz/eslint-config-react": "^0.0.3",
    "@znemz/prettier-config": "^0.0.1",
    "babel-cli": "6",
    "babel-core": "6",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^9.1.0",
    "babel-plugin-transform-class-properties": "6",
    "babel-plugin-transform-object-rest-spread": "6",
    "babel-preset-env": "^1.7",
    "cesium": "^1.0.0",
    "css-loader": "^6.7.2",
    "eslint": "^8.27.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-react": "^7.31.10",
    "http-server": "^14.1.1",
    "jest": "29",
    "jest-extended": "^3.1",
    "prettier": "^2.7.1",
    "rollup": "^3.3.0",
    "rollup-plugin-babel": "^3",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-json": "^3.1.0",
    "rollup-plugin-less": "^1.1.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "sort-package-json": "^2.1.0",
    "string-replace-loader": "^3.1.0",
    "style-loader": "^3.3.1",
    "webpack": "5",
    "webpack-cli": "^5.0.0"
  },
  "authors": [
    "Alberto Acevedo",
    "Nick McCready",
    "cmcleese"
  ],
  "browser_": "dist/browser.js",
  "mainCss": "dist/index.css"
}
